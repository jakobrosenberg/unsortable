{
  "name": "unsortable",
  "version": "0.1.7",
  "description": "Headless drag-and-drop library for nested sorting using state instead of DOM mutations.",
  "main": "lib/index.js",
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./lib/index.js",
    "./svelte": "./svelte/index.js",
    "./vue": "./vue/index.js"
  },
  "scripts": {
    "release": "release-it",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "drag-and-drop",
    "sortable",
    "nested",
    "headless",
    "state-driven",
    "reactive",
    "sort",
    "reorder",
    "javascript",
    "library",
    "UI logic",
    "Svelte",
    "SvelteKit",
    "agnostic",
    "vanilla js"
  ],
  "author": "jakobrosenberg@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@dnd-kit/dom": "^0.1.18"
  },
  "optionalDependencies": {
    "svelte": "^5.0.0",
    "vue": "^3.0.0"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}",
      "tagName": "v${version}",
      "requireCleanWorkingDir": false
    },
    "plugins": {
      "release-it-pnpm": {}
    }
  }
}
